{
    "name": "side-staker-connect-4",
    "description": "Connect 4 to win! matching side ways in a 7x7 grid.",
    "version": "0.0.1",
    "type": "module",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/geiz/side-stacker-game.git"
    },
    "author": "David Shi",
    "license": "MIT",
    "licenseUrl": "http://www.opensource.org/licenses/mit-license.php",
    "bugs": {
        "url": "https://github.com/geiz/side-stacker-game/issues"
    },
    "homepage": "https://github.com/geiz/side-stacker-game",
    "keywords": [
        "phaser",
        "phaser3",
        "react",
        "vite",
        "connect4"
    ],
    "scripts": {
        "dev": "node log.js dev & vite --config vite/config.dev.mjs",
        "build": "node log.js build & vite build --config vite/config.prod.mjs",
        "dev-nolog": "vite --config vite/config.dev.mjs",
        "build-nolog": "vite build --config vite/config.prod.mjs",
        "server": "python backend/server.py",
        "start": "concurrently \"npm run server\" \"npm run dev\""
    },
    "dependencies": {
        "phaser": "^3.88.2",
        "react": "^18.3.1",
        "react-dom": "^18.3.1"
    },
    "devDependencies": {
        "@types/react": "^18.3.3",
        "@types/react-dom": "^18.3.0",
        "@vitejs/plugin-react": "^4.3.1",
        "eslint": "^8.57.0",
        "eslint-plugin-react": "^7.34.2",
        "eslint-plugin-react-hooks": "^4.6.2",
        "eslint-plugin-react-refresh": "^0.4.7",
        "terser": "^5.28.1",
        "vite": "^5.3.1"
    }
}